    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "enzyme": "^2.9.1",
    "express": "^4.15.3",
    "jsdom": "^11.1.0",
    "koa-webpack-middleware": "^1.0.7",
    "react-test-renderer": "^15.6.1",
    "redux-devtools-extension": "^2.13.2",
    "webpack": "^3.3.0"
  },
  "dependencies": {
    "mocha": "^3.4.2",
